    "def imc_classificacao(valor):\n",
    "    if valor['IMC'] <18.5:\n",
    "        return 'Sobpeso'\n",
    "    if  (valor['IMC'] >= 18.5) & (valor['IMC'] <=24.9):\n",
    "        return 'Normal'\n",
    "    if  (valor['IMC'] >= 25) & (valor['IMC'] <=29.9):\n",
    "        return 'Sobrepeso'\n",
    "    if  (valor['IMC'] >= 30.0) & (valor['IMC'] <=39.9):\n",
    "        return 'Obesidade Grau I'\n",
    "    if valor['IMC'] >=40.0:\n",
    "        return 'Obesidade Grau II'"
   ]
